{
  "name": "ethan-demo-sdk",
  "version": "0.0.2",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EthanOK/ethan-demo-sdk.git"
  },
  "homepage": "https://github.com/EthanOK/ethan-demo-sdk",
  "keywords": [
    "typescript",
    "sdk"
  ],
  "author": "ethan",
  "license": "MIT",
  "description": "This is a demo for typescript sdk",
  "files": [
    "dist"
  ],
  "dependencies": {
    "ethers": "^6.15.0"
  },
  "devDependencies": {
    "@types/chai": "^5.2.2",
    "@types/mocha": "^10.0.10",
    "chai": "^5.2.1",
    "dotenv": "17.2.0",
    "eslint": "9.31.0",
    "husky": "^9.1.7",
    "lint-staged": "16.1.2",
    "mocha": "^11.7.1",
    "prettier": "3.6.2",
    "standard-version": "^9.5.0",
    "ts-node": "^10.9.2",
    "tsup": "8.5.0",
    "typedoc": "0.28.7",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.18.0"
  },
  "lint-staged": {
    "{src,tests}/**/*.{js,ts,tsx,json,css,md}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "scripts": {
    "prepare": "husky install",
    "prettier": "prettier --write \"{src,tests}/**/*.{js,ts,tsx,json,css,md}\"",
    "typedoc": "typedoc src/index.ts",
    "build": "tsup ./src/index.ts --format esm,cjs --dts",
    "link": "npm link && npm link ts-sdk-demo",
    "lint": "eslint \"{src,tests}/**/*.{js,ts,tsx}\"",
    "lint:fix": "eslint \"{src,tests}/**/*.{js,ts,tsx}\" --fix",
    "test": "yarn run mocha -p ./tsconfig.json -t 1000000 tests/**.spec.ts",
    "release": "standard-version && git push --follow-tags",
    "release:publish": "npm publish"
  },
  "packageManager": "yarn@4.9.2"
}
